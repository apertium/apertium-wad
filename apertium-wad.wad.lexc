%[%+3du%]:sur%> VerbRoot ;
%[%+3plhum%]:set%> VerbRoot ;
%[%+3plnh%]:si%> VerbRoot ;

LEXICON Verb

%<v%>%<p1%>%<du%>%<incl%>%[%-1incdu%]: # ;
%<v%>%<p1%>%<pl%>%<incl%>%[%-1incpl%]: # ;
%<v%>%<p1%>%<sg%>%[%-1sg%]:i # ;
%<v%>%<p1%>%<du%>%<excl%>%[%-1excdu%]: # ;
%<v%>%<p1%>%<pl%>%<excl%>%[%-1excpl%]: # ;
